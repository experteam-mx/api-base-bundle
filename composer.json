{
    "name": "experteam/api-base-bundle",
    "description": "Base functionality bundle for APIs",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Experteam",
            "email": "info@experteam.com.ec"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "doctrine/orm": "2.7.*",
        "doctrine/doctrine-bundle": "2.0.*",
        "antishov/doctrine-extensions-bundle": "^1.4",
        "symfony/form": "5.1.*",
        "symfony/dependency-injection": "5.1.*",
        "symfony/http-kernel": "5.1.*",
        "symfony/serializer": "5.1.*",
        "symfony/security-bundle": "5.1.*",
        "symfony/validator": "5.1.*",
        "predis/predis": "^1.1",
        "nelmio/api-doc-bundle": "^4.0",
        "friendsofsymfony/rest-bundle": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Experteam\\ApiBaseBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Experteam\\ApiBaseBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/flex": "^1.11",
        "symfony/dotenv": "5.1.*",
        "symfony/phpunit-bridge": "5.1.*",
        "symfony/config": "5.1.*"
    }
}
